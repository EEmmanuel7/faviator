{
  "name": "faviator",
  "version": "1.1.2",
  "description": "A simple easy favicon generator.",
  "main": "lib/index.js",
  "scripts": {
    "preversion": "npm test && $EDITOR CHANGELOG.md",
    "version": "git add -A",
    "postpublish": "git push --all && git push --tags",
    "\n# TESTING SCRIPTS:": "",
    "test": "nyc mocha --recursive",
    "test:watch": "mocha --recursive --watch",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "lint:js": "eslint .",
    "\n# HUSKY GIT HOOKS:": "",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "dependencies": {
    "@faviator/create-svg-favicon": "^1.1.0",
    "commander": "^2.15.1",
    "convert-svg-to-jpeg": "^0.4.0",
    "convert-svg-to-png": "^0.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@ycm.jason/eslint-config": "^1.2.3",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "mocha": "^5.2.0",
    "nyc": "^12.0.1",
    "proxyquire": "^2.0.1",
    "sinon": "^5.0.10",
    "validate-commit": "^3.4.0"
  },
  "keywords": [
    "favicon",
    "generator",
    "generate",
    "icon"
  ],
  "author": "Jason Yu <me@ycmjason.com>",
  "license": "ISC",
  "lint-staged": {
    "*.js": "npm run lint:js"
  },
  "bin": {
    "faviator": "./bin/faviator.js"
  },
  "directories": {
    "example": "example",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faviator/faviator.git"
  },
  "bugs": {
    "url": "https://github.com/faviator/faviator/issues"
  },
  "homepage": "https://github.com/faviator/faviator#readme"
}
